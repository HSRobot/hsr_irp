<launch>
  <arg name="sim" default="false" />
    
  <!-- Remap follow_joint_trajectory -->
  <remap if="$(arg sim)" from="/follow_joint_trajectory" to="/arm_trajectory_controller/follow_joint_trajectory"/>
  
  <!-- Launch moveit -->
  <include file="$(find co602_moveit_config)/launch/move_group.launch">
  </include>
</launch>

